
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title | Prestige YoYo</title>
    @Url.StyleSheet("~/Content/Prestige.css",
                    "~/Content/jquery-ui-1.9.1.custom.min.css",
                    "~/Content/ui.jqgrid.css")
    @Url.Script("~/Scripts/jquery-1.7.2.min.js",
                "~/Scripts/jquery-ui-1.8.18.min.js",
                "~/Scripts/modernizr-1.7.min.js",
                "~/Scripts/grid.locale-en.js",
                "~/Scripts/jquery.jqGrid.src.js")

    @RenderSection("head", false)
</head>
<body>
    <header>
        <div id="title">
        @if (ViewBag.Title != null)
        {
            <h1>Prestige YoYo &mdash; @ViewBag.Title</h1>
        }
        else
        { 
            <h1>Prestige YoYo</h1>
        }
        </div>
        <div id="logindisplay">
            @Html.Partial("Logo")
        </div>
        <nav>
            @Html.NavigationBar()
        </nav>
    </header>
    <section id="main">
        @RenderBody()
    </section>
    <footer>
        <div>
            <span>[ <a href="http://www.hrsdc.gc.ca/eng/home.shtml" target="_blank">HRSDC</a> ]</span>
            <span>[ <a href="http://www.labour.gov.on.ca/english/" target="_blank">Ministry of Labour</a> ]</span>
            <span>[ <a href="http://www.olrb.gov.on.ca/english/homepage.htm" target="_blank">Labour Relations Board</a> ]</span>
        <div>Codeora &copy; 2012</div>
    </footer>
</body>
</html>
