
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    @Url.StyleSheet("~/Content/Prestige.css")
    @Url.Script("~/Scripts/jquery-1.7.1.min.js",
                "~/Scripts/modernizr-1.7.min.js")

    @RenderSection("head", false)
</head>
<body>
    <header>
        <div id="title">
            <h1>Prestige Yo-Yo</h1>
        </div>
        <div id="logindisplay">
            @Html.Partial("Logo")
        </div>
        <nav>
            <ul id="menu">
                <li>@Html.ActionLink("Reports", "Index", "Report")</li>
                <li>@Html.ActionLink("Administration", "Index", "Account")</li>
            </ul>
        </nav>
    </header>
    <section id="main">
        @RenderBody()
    </section>
    <footer>
        <div>I am a footer...</div>
        <div>Three Guys Coding &copy; 2012</div>
    </footer>
</body>
</html>
